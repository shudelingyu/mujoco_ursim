{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug cartesian_admittance",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/cartesian_admittance/cartesian_admittanced",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
